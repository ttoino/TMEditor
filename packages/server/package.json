{
  "name": "server",
  "version": "0.1.0",
  "license": "BSD 3-Clause License",
  "private": true,
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "webpack --mode development",
    "run:nodemon": "NODE_ENV=development nodemon --inspect --watch dist/index.js -r source-map-support/register dist/index.js",
    "build": "webpack --mode production",
    "test": "cd ./test/assets/firebase-emulator && firebase emulators:exec --import=./data \"npm run jest\"",
    "jest": "jest",
    "test-watch": "cd ./test/assets/firebase-emulator && firebase emulators:exec --import=./data \"npm run test-watch-exec\"",
    "test-watch-exec": "node --inspect $(npm bin)/jest --watch --no-cache --runInBand"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/test/setupJest.ts"
    ],
    "testTimeout": 60000
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^18.0.2",
    "axios": "^0.27.2",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "express": "~4.18.1",
    "express-session": "^1.17.3",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "js-yaml": "^4.1.0",
    "keycloak-connect": "^18.0.2",
    "lodash.pick": "^4.4.0",
    "log4js": "^6.5.2",
    "mariadb": "^3.0.0",
    "nodemon": "^2.0.18",
    "redis": "^4.1.0",
    "sequelize": "^6.21.2",
    "sequelize-auto": "^0.8.8",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.0.8",
    "ts-loader": "^9.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.pick": "^4.4.7",
    "@types/node": "^18.0.0",
    "@types/nodemon": "^1.19.1",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "axios-mock-adapter": "^1.21.1",
    "babel-jest": "^28.1.2",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "firebase-tools": "^10.9.2",
    "jest": "^28.1.2",
    "typescript": "^4.7.4"
  }
}
